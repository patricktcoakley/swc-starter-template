{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "preserveConstEnums": true,
    "allowUnreachableCode": false,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
